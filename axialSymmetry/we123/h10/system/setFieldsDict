boxToCell
{
  box (0 0.012 0) (0.001 0.014 0.01);
  fieldValues
    (
     volScalarFieldValue alpha.water 1
     volVectorFieldValue U (0 -1.69 0)
     );
}
 boxToCell // Basin depth
    {
	box (0 0 0) (0.100 0.010 0.010);
        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }
);


